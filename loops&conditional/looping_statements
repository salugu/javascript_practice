let i=5;
do {
   
    if(i==6)
    {
        break;
    }
     console.log("saicharan:",i);
     i--;
}while(i>0);
console.log("end");


let increament=0;
do{
    console.log("sai:",increament);
    i++;

}while(i<0);
console.log("end ");


let index=5;
do{
    console.log(index);

}while(--index);




